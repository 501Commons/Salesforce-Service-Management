    <fields>
        <fullName>C501_HH_Income_Level__c</fullName>
        <description>Module4</description>
        <externalId>false</externalId>
        <formula>IF ( 
 AND ( NOT ( ISBLANK ( C501_Annual_Income__c )), NOT ( ISBLANK (  C501_Household_Size__c ))),
    C501_Annual_Income__c / 
   ( $Setup.C501_HH_Income_Settings__c.C501_Base_Level__c + 
   (( C501_Household_Size__c - 1 )*  $Setup.C501_HH_Income_Settings__c.C501_Additional_Amount__c )),
0 )


/* (Base_Threshold + ((NumberInHH-1)*AdditionalValuePerPerson) */</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Formula - based on Household Size and Annual Income. Used in Module4 reporting</inlineHelpText>
        <label>HH Income Level</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Percent</type>
    </fields>
