<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <fields>
        <fullName>Contact_Link__c</fullName>
        <description>Detects if User is in Lightning vs. Classic and creates link to Contact</description>
        <externalId>false</externalId>
        <formula>HYPERLINK(
    IF($User.UIThemeDisplayed = &quot;Theme4d&quot;,
       &quot;/one/one.app#/sObject/&quot; &amp;  npe5__Contact__r.Id  &amp; &quot;/view&quot;,
       &quot;/&quot; &amp; npe5__Contact__r.Id),
    npe5__Contact__r.FirstName &amp; &quot; &quot; &amp; npe5__Contact__r.LastName, &quot;_top&quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Contact Link</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Contact_Name__c</fullName>
        <description>Text formula field created so the Contact Name can appear in related lists in Lightning Experience and Salesforce1</description>
        <externalId>false</externalId>
        <formula>npe5__Contact__r.FirstName &amp; &quot; &quot; &amp; npe5__Contact__r.LastName</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Auto-created based on Contact First &amp; Last Name</inlineHelpText>
        <label>Contact Name</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
</CustomObject>
